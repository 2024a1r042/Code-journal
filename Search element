#include<stdio.h>
#include<stdlib.h>
int main(){
    system("cls");
int n,flag=0;
printf("Enter Array size:");
scanf("%d",&n);
int arr[n];
printf("Enter %d elements:",n);
for(int i=0;i<n;i++){
    scanf("%d",&arr[i]);
}
int target;
printf("Enter searching element:");
scanf("%d",target);
for(int i=0;i<n;i++){
    if(arr[i]==target){
        flag=1;
        break;
        }    }
        if(flag==1)
printf("Element found.");
else
printf("Element not found.");
return 0;
}
